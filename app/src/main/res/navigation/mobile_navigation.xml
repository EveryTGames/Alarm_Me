<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_settings">

    <fragment
        android:id="@+id/nav_rules"
        android:name="com.etgames.alarmme.ui.rules.RulesFragment"
        android:label="Rules"
        tools:layout="@layout/fragment_rules" >
        <action
            android:id="@+id/action_nav_rules_to_nav_toggledapps"
            app:destination="@id/nav_toggledapps" />
    </fragment>

    <fragment
        android:id="@+id/nav_alarms"
        android:name="com.etgames.alarmme.ui.alarms.AlarmsFragment"
        android:label="Alarms"
        tools:layout="@layout/fragment_alarms" />
    <fragment
        android:id="@+id/nav_settings"
        android:name="com.etgames.alarmme.ui.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/nav_toggledapps"
        android:name="com.etgames.alarmme.ui.toggledApps.toggledAppsFragment"
        android:label="Toggled Apps"
        tools:layout="@layout/fragment_toggledapps" >
        <action
            android:id="@+id/action_nav_toggledapps_to_nav_rules"
            app:destination="@id/nav_rules" />
    </fragment>
</navigation>